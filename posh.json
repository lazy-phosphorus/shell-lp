{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#E74C3C",
                    "foreground": "#F1C40F",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": "  ",
                    "type": "root"
                },
                {
                    "background": "#9B59B6",
                    "foreground": "#E8EAEE",
                    "powerline_symbol": "",
                    "properties": {
                        "command": "[ $MSYS_ENV ] && (echo $MSYS_ENV)",
                        "interpret": false,
                        "shell": "bash"
                    },
                    "style": "powerline",
                    "template": "{{ if .Output }} {{ .Output }} {{ end }}",
                    "type": "command"
                },
                {
                    "background": "#2ECC71",
                    "foreground": "#2C3E50",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": " {{ .UserName }}@{{ .HostName }} ",
                    "type": "session"
                },
                {
                    "background": "#566573",
                    "foreground": "#E8EAEE",
                    "powerline_symbol": "",
                    "properties": {
                        "style": "full"
                    },
                    "style": "powerline",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "#F39C12",
                    "foreground": "#2C3E50",
                    "powerline_symbol": "",
                    "properties": {
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "template": " {{ .HEAD }} {{ if .Behind }}{{ .Behind }} {{ end }}{{ if .Ahead }}{{ .Ahead }} {{ end }}{{ if .Working.Changed }}{{ .Working.String }} {{ end }}{{ if .Staging.Changed }}{{ .Staging.String }} {{ end }}",
                    "type": "git"
                },
                {
                    "background": "#16A085",
                    "foreground": "#E8EAEE",
                    "powerline_symbol": "",
                    "properties": {
                        "always_enabled": true,
                        "style": "amarillo",
                        "threshold": 0
                    },
                    "style": "powerline",
                    "template": " {{ .FormattedMs }} ",
                    "type": "executiontime"
                },
                {
                    "background": "#E74C3C",
                    "foreground": "#E8EAEE",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": " {{ reason .Code }}({{ .Code }}) ",
                    "type": "status"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "background": "#1F618D",
                    "foreground": "#E8EAEE",
                    "powerline_symbol": "",
                    "properties": {
                        "cache_version": true,
                        "command": "cmake --version 2>&1 | awk '{print $114}'; [ $? -eq 0 ] && [[ 0 -ne $(ls *.h *.c *.cpp *.hpp CMakeLists.txt 2>&1 | grep -v 'ls:' | wc -l) ]] && (cmake --version | grep version | awk '{print $3}')",
                        "interpret": false,
                        "shell": "bash"
                    },
                    "style": "powerline",
                    "template": "{{ if .Output }} CMake {{ .Output }} {{ end }}",
                    "type": "command"
                },
                {
                    "background": "#FAD7A0",
                    "foreground": "#2C3E50",
                    "powerline_symbol": "",
                    "properties": {
                        "cache_version": true,
                        "command": "gcc --version 2>&1 | awk '{print $114}'; [ $? -eq 0 ] && [[ 0 -ne $(ls *.h *.c *.cpp *.hpp CMakeLists.txt 2>&1 | grep -v 'ls:' | wc -l) ]] && (gcc --version | grep gcc | awk -v FS=') ' '{print $2}')",
                        "interpret": false,
                        "shell": "bash"
                    },
                    "style": "powerline",
                    "template": "{{ if .Output }} GCC {{ .Output }} {{ end }}",
                    "type": "command"
                },
                {
                    "background": "#16A085",
                    "foreground": "#2C3E50",
                    "powerline_symbol": "",
                    "properties": {
                        "cache_version": true,
                        "command": "clang --version 2>&1 | awk '{print $114}'; [ $? -eq 0 ] && [[ 0 -ne $(ls *.h *.c *.cpp *.hpp CMakeLists.txt 2>&1 | grep -v 'ls:' | wc -l) ]] && (clang --version | grep clang | awk -v FS='version ' '{print $2}')",
                        "interpret": false,
                        "shell": "bash"
                    },
                    "style": "powerline",
                    "template": "{{ if .Output }} LLVM {{ .Output }} {{ end }}",
                    "type": "command"
                },
                {
                    "background": "#76D7C4",
                    "foreground": "#2C3E50",
                    "powerline_symbol": "",
                    "properties": {
                        "cache_version": true,
                        "command": "go version 2>&1 | awk '{print $114}'; [ $? -eq 0 ] && [[ 0 -ne $(ls *.go go.mod 2>&1 | grep -v 'ls:' | wc -l) ]] && (go version | awk '{print $3}' | awk -v FS='go' '{print $2}')",
                        "interpret": false,
                        "shell": "bash"
                    },
                    "style": "powerline",
                    "template": "{{ if .Output }} Go {{ .Output }} {{ end }}",
                    "type": "command"
                },
                {
                    "background": "#6CA35E",
                    "foreground": "#E8EAEE",
                    "powerline_symbol": "",
                    "properties": {
                        "cache_version": true,
                        "command": "node --version 2>&1 | awk '{print $114}'; [ $? -eq 0 ] && [[ 0 -ne $(ls *.mjs *.js *.ts *.mts package.json tsconfig.json node_modules *.jsx *.tsx 2>&1 | grep -v 'ls:' | wc -l) ]] && (node --version | awk -v FS='v' '{print $2}')",
                        "interpret": false,
                        "shell": "bash"
                    },
                    "style": "powerline",
                    "template": "{{ if .Output }} Node.js {{ .Output }} {{ end }}",
                    "type": "command"
                },
                {
                    "background": "#F39C12",
                    "foreground": "#2C3E50",
                    "powerline_symbol": "",
                    "properties": {
                        "cache_version": true,
                        "command": "java --version 2>&1 | awk '{print $114}'; [ $? -eq 0 ] && [[ 0 -ne $(ls pom.xml *.java 2>&1 | grep -v 'ls:' | wc -l) ]] && (java --version | grep openjdk | awk '{print $2}')",
                        "interpret": false,
                        "shell": "bash"
                    },
                    "style": "powerline",
                    "template": "{{ if .Output }} Java {{ .Output }} {{ end }}",
                    "type": "command"
                },
                {
                    "background": "#2471A3",
                    "foreground": "#E8EAEE",
                    "powerline_symbol": "",
                    "properties": {
                        "cache_version": true,
                        "command": "python --version 2>&1 | awk '{print $114}'; [ $? -eq 0 ] && [[ 0 -ne $(ls *.py *.ipynb 2>&1 | grep -v 'ls:' | wc -l) ]] && (python --version | awk '{print $2}')",
                        "interpret": false,
                        "shell": "bash"
                    },
                    "style": "powerline",
                    "template": "{{ if .Output }} Python {{ .Output }} {{ end }}",
                    "type": "command"
                },
                {
                    "foreground": "#E8EAEE",
                    "style": "plain",
                    "template": "❯",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}